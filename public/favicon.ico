<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" fill="none">
  <!-- Main compass body -->
  <circle cx="16" cy="16" r="14" fill="#374151" stroke="#4B5563" stroke-width="0.8"/>
  
  <!-- Inner compass face -->
  <circle cx="16" cy="16" r="12" fill="#F9FAFB"/>
  
  <!-- Cardinal marks -->
  <g stroke="#374151" stroke-width="1.2">
    <line x1="16" y1="6" x2="16" y2="8"/>
    <line x1="26" y1="16" x2="24" y2="16"/>
    <line x1="16" y1="26" x2="16" y2="24"/>
    <line x1="6" y1="16" x2="8" y2="16"/>
  </g>
  
  <!-- Compass needle -->
  <g>
    <!-- North pointer (red) -->
    <polygon points="16,8 14,14 16,13 18,14" fill="#DC2626"/>
    
    <!-- South pointer (white) -->
    <polygon points="16,24 18,18 16,19 14,18" fill="#F9FAFB" stroke="#374151" stroke-width="0.4"/>
  </g>
  
  <!-- Center point -->
  <circle cx="16" cy="16" r="1.6" fill="#374151"/>
  <circle cx="16" cy="16" r="0.8" fill="#F9FAFB"/>
</svg> 